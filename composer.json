{
  "name": "alessandro-amos/cycle-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "cycle",
    "cycle-bundle",
    "dbal",
    "orm",
    "database",
    "persistence"
  ],
  "description": "Cycle DBAL & ORM Bundle for the Symfony Framework.",
  "autoload": {
    "psr-4": {
      "Alms\\Bundle\\CycleBundle\\": "./"
    }
  },
  "authors": [
    {
      "name": "Slince",
      "email": "taosikai@yeah.net"
    },
    {
      "name": "Alessandro Amos",
      "email": "alessandroamos@outlook.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "cycle/annotated": "^4.1",
    "cycle/entity-behavior": "^1.3",
    "cycle/orm": "^2.8",
    "cycle/schema-migrations-generator": "^2.2",
    "cycle/schema-renderer": "^1.2",
    "symfony/cache": "^7.0",
    "symfony/config": "^7.0",
    "symfony/console": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/deprecation-contracts": "^3.5",
    "symfony/framework-bundle": "^7.0",
    "symfony/service-contracts": "^3.5",
    "symfony/uid": "^7.0"
  },
  "suggest": {
    "ext-pdo": "*",
    "ext-dom": "*",
    "symfony/uid": "^7.0"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/flex": true
    },
    "sort-packages": true
  }
}
